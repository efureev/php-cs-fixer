{
    "name": "feugene/php-cs-fixer",
    "description": "A tool to automatically fix PHP code style",
    "keywords": [
        "codestyle",
        "cs",
        "fixer"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eugene Fureev",
            "homepage": "https://github.com/efureev"
        },
        {
            "name": "Avto Develop",
            "homepage": "https://github.com/avto-dev"
        }
    ],
    "require": {
        "php": "^5.6 || >=7.0",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "require-dev": {},
    "config": {
        "sort-packages": true
    },
    "support": {
        "issues": "https://github.com/efureev/php-cs-fixer/issues",
        "source": "https://github.com/efureev/php-cs-fixer"
    },
    "bin": [
        "cs-fix"
    ]
}
